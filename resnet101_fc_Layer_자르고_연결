      "cell_type": "code",
      "source": [
        "class MyResNet101(nn.Module):\n",
        "    def __init__(self, pretrained_model):\n",
        "        super(MyResNet101, self).__init__()\n",
        "        self.backbone = nn.Sequential(*list(pretrained_model.children())[:-2])  # 최종 풀링 레이어까지 포함\n",
        "\n",
        "        # 파라미터 고정(frozen)\n",
        "        for param in self.backbone.parameters():\n",
        "            param.requires_grad = False\n",
        "\n",
        "        self.extra_layer = nn.Sequential(\n",
        "            nn.Linear(2048, 1024),\n",
        "            nn.ReLU(True),\n",
        "            nn.Dropout(0.25),\n",
        "            nn.Linear(1024, 512),\n",
        "            nn.ReLU(True),\n",
        "            nn.Dropout(0.25),\n",
        "            nn.Linear(512, 4)\n",
        "        )\n",
        "\n",
        "    def forward(self, x):\n",
        "        x = self.backbone(x)\n",
        "        x = torch.flatten(x, 1)  # 평탄화\n",
        "        x = self.extra_layer(x)\n",
        "        return x\n"
      ],
      "metadata": {
        "id": "PhIfdU4E_b1W"
      },
      "execution_count": null,
      "outputs": []
